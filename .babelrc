{
    "presets": ["babel-preset-expo"],
    "plugins": [
		["@babel/plugin-proposal-decorators", {"legacy": true}],
		[
			"module-resolver",
			{
				"cwd": "babelrc",
				"root": ["./src"],
				"extensions": [".js", ".ios.js", ".android.js"],
				"alias": {
					"_components": "./src/components",
					"_screens": "./src/screens",
					"_constants": "./src/constants",
					"_stores": "./src/stores",
					"_language": "./src/language"
				}
			}]
	]

 }